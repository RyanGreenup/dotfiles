#!/bin/bash

# Convert the clipboard to SVG using tex2svg
tex="$(xclip -sel clip -o | tr -d '\n')"
name="/tmp/$(echo ${tex} | xxhsum -H0 | cut -f 1 -d ' ').svg"

echo ${tex}
tex2svg "${tex}" > "${name}"

# Copy the filename to the clipboard
echo "${name}" | xclip -sel clip

# Print this
bat "${0:-}" -r 3:10
